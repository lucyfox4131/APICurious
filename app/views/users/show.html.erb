<div class="container">
  <h1><%= @user.name %></h1>
  <%= image_tag @user.avatar %>
  <h2>You Have <%= @user.followers.count %> followers </h2>
  <h2>You Are Following <%= @user.following.count %> people </h2>
  <h2>You Have Starred <%= @user.starred.count %> repos </h2>

<ul>
  <% @user.repos.each do |repo| %>
    <li><%= link_to repo["name"], repo["html_url"] %></li>
  <% end %>
</ul>

<ul>
  <h2>Organizations:</h2>
  <% @user.organizations.each do |org| %>
  <li><%= org["name"] %></li>
  <% end %>
</ul>

<ul>
  <h2>Events For Logged In User</h2>
  <% @user.events.each do |event| %>
    <li>Event Type: <%= event["type"] %></li>
  <% end %>
</ul>

<ul>
  <h2>Events For People You Follow: </h2>
  <% @user.following_events.each do |user_events| %>
  <!-- <li><h4>Events for <%#= user_events["actor"]["login"] %></h4></li> -->
    <!-- <%# user_events.each do |event| %> -->
      <!-- Event Type: <%#= event["type"] %> -->
    <!-- <%# end %> -->
  <% end %>
</ul>

</div>
